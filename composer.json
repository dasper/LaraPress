{
  "name": "dasper/lara-press",
  "description": "Wordpress compatible package utilizing Laravel components",
  "keywords": [
    "php",
    "wordpress",
    "laravel"
  ],
  "require": {
    "php": ">=5.5.9",
    "vlucas/phpdotenv": "~1.0",
    "illuminate/database": "5.1.*",
    "illuminate/support": "5.1.*",
    "illuminate/events": "5.1.*",
    "diff/diff": "2.0.0",
    "james-heinrich/getid3": "1.9.9",
    "simplepie/simplepie": "1.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "authors": [
    {
      "name": "Will Shaw",
      "email": "dasper@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "LaraPress\\": "src/"
    }
  },
  "suggest": {
    "nikic/fast-route": "Customize routing and override WP defaults"
  },
  "minimum-stability": "dev"
}
